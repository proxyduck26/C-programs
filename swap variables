#include <iostream>
using namespace std;
int main()
{
    int a,b,temp;
    cout<< "Enter any two variables"<<endl;
    cin<<a<<b;
    cout<<"Before swapping, variable 1 = " << a <<" and variable 2 = "<< b <<endl;
    temp=a; 
    a=b; 
    b=temp;
    cout<<"After swapping, variable 1 = " << a <<" and variable 2 = "<< b;
    
    return 0;
}
